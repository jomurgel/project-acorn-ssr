{
  "name": "project-acorn-ssr",
  "version": "0.0.1",
  "description": "A Vue.js SPA built around the WordPress REST API with Vuex, Vue Router, Axios and SSR.",
  "author": "jomurgel <dev@jomurgel.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jomurgel/project-acorn-ssr"
  },
  "keywords": [
    "acorn",
    "axios",
    "jomurgel",
    "rest api",
    "ssr",
    "starter",
    "vue",
    "vue-router",
    "vuex",
    "webpack",
    "wordpress",
    "spa"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jomurgel/project-acorn-ssr/issues"
  },
  "homepage": "https://github.com/jomurgel/project-acorn-ssr",
  "scripts": {
    "dev": "node server",
    "svg": "node build/svg-sprite.js",
    "go": "npm run build && npm run start",
    "start": "cross-env NODE_ENV=production node server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build": "rimraf dist && npm run svg && npm run build:client && npm run build:server"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "build": "^0.1.4",
    "compression": "^1.7.2",
    "express": "^4.15.3",
    "lru-cache": "^4.1.1",
    "run": "^1.4.0",
    "sanitize.scss": "^1.0.0",
    "serve-favicon": "^2.4.3",
    "skeleton-scss": "^2.0.4",
    "start": "^5.1.0",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "^0.11.4",
    "vue": "^2.5.13",
    "vue-analytics": "^5.9.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "svg-sprite": "^1.3.7",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
